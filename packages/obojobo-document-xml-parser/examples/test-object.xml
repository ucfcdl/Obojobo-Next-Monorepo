							<actions>
								<action type="nav:lock" />
							</actions>
						</trigger>
					</triggers>
				</ActionButton>
				<ActionButton>
					<textGroup>
						<t>Unlock navigation (<code>nav:unlock</code>)</t>
					</textGroup>
					<triggers>
						<trigger type="onClick">
							<actions>
								<action type="nav:unlock" />
							</actions>
						</trigger>
					</triggers>
				</ActionButton>
				<ActionButton>
					<textGroup>
						<t>Open navigation (<code>nav:open</code>)</t>
					</textGroup>
					<triggers>
						<trigger type="onClick">
							<actions>
								<action type="nav:open" />
							</actions>
						</trigger>
					</triggers>
				</ActionButton>
				<ActionButton>
					<textGroup>
						<t>Close navigation (<code>nav:close</code>)</t>
					</textGroup>
					<triggers>
						<trigger type="onClick">
							<actions>
								<action type="nav:close" />
